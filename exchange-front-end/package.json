{
  "name": "@exchange-front-end/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "start:auth": "nx serve --project=auth --port=4200",
    "start:admin": "nx serve --project=admin --port=4201",
    "build": "nx build",
    "build:auth:test": "nx build --project=auth --c=testin --prod --skip-nx-cache",
    "build:admin:test": "nx build --project=admin --c=testin --prod --skip-nx-cache --base-href /admin/",
    "build:auth:stage": "nx build --project=auth --c=staging --prod --skip-nx-cache",
    "build:admin:stage": "nx build --project=admin --c=staging --prod --skip-nx-cache --base-href /admin/",
    "build:auth:preprod": "nx build --project=auth --c=preproduction --prod --skip-nx-cache",
    "build:admin:preprod": "nx build --project=admin --c=preproduction --prod --skip-nx-cache",
    "build:auth:prod": "nx build --project=auth --c=production --prod --skip-nx-cache",
    "build:admin:prod": "nx build --project=admin --c=production --prod --skip-nx-cache",
    "test": "nx test --codeCoverage",
    "test:auth": "nx test --project=auth --codeCoverage",
    "test:admin": "nx test --project=admin --codeCoverage",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "npx nx affected:test --parallel --skip-nx-cache --codeCoverage",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prepare": "cd .. && npx husky install exchange-front-end/.husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~16.0.0",
    "@angular/common": "~16.0.0",
    "@angular/compiler": "~16.0.0",
    "@angular/core": "~16.0.0",
    "@angular/forms": "~16.0.0",
    "@angular/platform-browser": "~16.0.0",
    "@angular/platform-browser-dynamic": "~16.0.0",
    "@angular/router": "~16.0.0",
    "@nx/angular": "16.3.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.0.0",
    "@angular-devkit/core": "~16.0.0",
    "@angular-devkit/schematics": "~16.0.0",
    "@angular-eslint/eslint-plugin": "~16.0.0",
    "@angular-eslint/eslint-plugin-template": "~16.0.0",
    "@angular-eslint/template-parser": "~16.0.0",
    "@angular/cli": "~16.0.0",
    "@angular/compiler-cli": "~16.0.0",
    "@angular/language-service": "~16.0.0",
    "@nx/cypress": "16.3.2",
    "@nx/eslint-plugin": "16.3.2",
    "@nx/jest": "16.3.2",
    "@nx/js": "16.3.2",
    "@nx/linter": "16.3.2",
    "@nx/web": "16.3.2",
    "@nx/workspace": "16.3.2",
    "@schematics/angular": "~16.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cypress": "^12.11.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-preset-angular": "~13.1.0",
    "lint-staged": "^14.0.1",
    "nx": "16.3.2",
    "nx-cloud": "latest",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.0.2"
  },
  "lint-staged": {
    "**/*.ts": [
      "nx format:write --uncommitted"
    ],
    "**/*.{html,scss}": []
  }
}
